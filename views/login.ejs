<% include head.ejs %>
    <div class="jumbotron">
      <h1 class="studybuddy">StudyBuddy</h1>
      <br />
      <form role="form" action="/login" method="post" style="max-width: 300px;">
        <div class="form-group">
          <input type="text" name="username" placeholder="Email" class="form-control">
        </div>
        <div class="form-group">
          <input type="password" name="password" placeholder="Password" class="form-control">
        </div>
        <button type="submit" class="btn btn-primary" style="margin-top:10px;">Login</button>&nbsp;
        <a href="/"><button type="button" class="btn btn-primary" style="margin-top:10px;">Cancel</button></a>
      </form>
      <h4 class="msg"><%- msg -%></h4>
    </div>
<% include foot.ejs %>